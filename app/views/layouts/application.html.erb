            <a href="#" class="close"></a>  
            <div class="shadow"></div>
            <div id="form_wrap">
              <form id="contact-form" action="javascript:alert('success!');">
                <div class="contact_img"></div>
                <div class="sep"></div>
                <p id="formstatus"></p>
                <label for="name"><cufon class="cufon cufon-canvas" alt="Name " style="width: 28px; height: 14px; "><canvas width="39" height="16" style="width: 39px; height: 16px; top: -1px; left: -2px; "></canvas><cufontext>Name </cufontext></cufon><cufon class="cufon cufon-canvas" alt=":" style="width: 3px; height: 14px; "><canvas width="14" height="16" style="width: 14px; height: 16px; top: -1px; left: -2px; "></canvas><cufontext>:</cufontext></cufon></label>
