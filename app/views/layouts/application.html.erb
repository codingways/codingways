                <label for="email"><cufon class="cufon cufon-canvas" alt="Email " style="width: 29px; height: 14px; "><canvas width="40" height="16" style="width: 40px; height: 16px; top: -1px; left: -2px; "></canvas><cufontext>Email </cufontext></cufon><cufon class="cufon cufon-canvas" alt=":" style="width: 3px; height: 14px; "><canvas width="14" height="16" style="width: 14px; height: 16px; top: -1px; left: -2px; "></canvas><cufontext>:</cufontext></cufon></label>
                <input type="text" name="email" value="" id="email"><br>
                <label for="email"><cufon class="cufon cufon-canvas" alt="Message " style="width: 43px; height: 14px; "><canvas width="54" height="16" style="width: 54px; height: 16px; top: -1px; left: -2px; "></canvas><cufontext>Message </cufontext></cufon><cufon class="cufon cufon-canvas" alt=":" style="width: 3px; height: 14px; "><canvas width="14" height="16" style="width: 14px; height: 16px; top: -1px; left: -2px; "></canvas><cufontext>:</cufontext></cufon></label>
                <textarea name="message" rows="" cols="" id="message"></textarea>
                <div class="bar"></div><input type="submit" name="submit" value="" class="button">
              </form>
            </div>
          </div><!--end contact form--> 

          <!--begin shadow effect--> 
          <div id="shadow"></div><!--end shadow effect-->


